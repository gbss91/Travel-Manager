<div id="<%= dom_id booking %>">

  <p>
    <strong>User:</strong>
    <%= booking.user_id %>
  </p>

  <p>
    <strong>Booked on:</strong>
    <%= booking.booked_on_date %>
  </p>

  <p>
    <strong>Trip date:</strong>
    <%= booking.origin %>
  </p>

  <p>
    <strong>Destination:</strong>
    <%= booking.destination %>
  </p>

  <p>
    <strong>Status:</strong>
    <%= booking.status %>
  </p>

  <p>
    <strong>Total price:</strong>
    <%= booking.total_price %>
  </p>

  <p>
    <strong>Currency:</strong>
    <%= booking.currency %>
  </p>

</div>
